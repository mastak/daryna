{

	/*
	 * настойчивые параметры
	 */

	"bitwise": false, // запретить использование битовых операций
	"camelcase": false, // требовать верблюжью нотацию имён переменных
	"curly": true, // запретить циклы и условия без фигурных скобок блока
	"eqeqeq": true, // запретить использование нестрогих сравнений
	"forin": true, // запретить перебор свойств объекта без фильтраций
	"immed": true, // требовать вызова функции внутри упаковки в круглые скобки
	"indent": 4, // количество пробелов в отступе (табуляции)
	"latedef": true, // запрещать использование переменной прежде чем она была определена
	"newcap": true, // требовать вызова конструктора метода с заглавной буквы
	"noarg": true, // запретить использование устаревших и медленных методов arguments
	"noempty": true, // запретить использование пустых блоков в коде
	"nonew": true, // запретить вызов конструктора метода без присвоения его переменной
	"plusplus": false, // запретить использование унарных операций
	"quotmark": "single", // предпочитаемый стиль кавычек
	"regexp": true, // запретить использование точки в регулярных выражениях
	"undef": true, // запретить использование не объявленных переменных
	"predef": [
		"window",
		"define",
		"require",
		"requirejs",
		"console",
		"document",
		"Image",
		"XMLHttpRequest",
		"FormData",
		"File",
		"DocumentTouch"
	], // Predefined global variables
	"unused": true, // запретить неиспользуемые переменные
	"strict": true, // запретить использование нестрого режима для функций
	"trailing": true, // запретить использование пробела в конце многострочной строки
	"maxparams": 4, // максимальное количество параметров функции
	"maxdepth": 3, // максимальная вложенность блоков функции
	// "maxstatements": 4, // ?
	// "maxcomplexity": 4, // ?
	"maxlen": 128, // максимально допустимая длина строки кода

	/*
	 * расслабляющие параметры
	 */
	"asi": false, // разрешить пропускать точку с запятой
	"boss": true, // разрешить присваивание там, где это не ожидается
	"debug": false, // разрешить вызовы debugger-отладчика
	"eqnull": false, // разрешить нестрогое сравнение null
	"es5": false, // разрешить ES5 возможности (get, set, ...)
	"esnext": false, // разрешить ESNext возможности (const, let, ...)
	"evil": false, // разрешить использование eval
	"expr": false, // разрешить выражения без присвоения переменной
	"funcscope": false, // разрешить использование переменных внутри функций, объявленных в других областях
	"globalstrict": true, // разрешить использование глобального строгого режима
	"iterator": false, // разрешить использование __iterator__
	"lastsemic": false, // разрешить пропускать точку с запятой не только в конце строки
	"laxbreak": false, // разрешить небезопасные окончания строк
	"laxcomma": false, // разрешить comma-first стиль кода
	"loopfunc": false, // разрешить объявлять функции внутри циклов
	"multistr": false, // разрешить многострочные строки
	"onecase": false, // разрешить switch с одним case
	"proto": false, // разрешить использование __proto__
	"regexdash": false, // разрешить незамаскированные тире в конце регулярных выражений
	"scripturl": false, // разрешить букмарклеты
	"smarttabs": false, // разрешить смешанные табуляции и пробелы
	"shadow": false, // разрешить объявление переменных, уже объявленных в областях выше
	"sub": false, // разрешить использовать []-нотацию вместо .-нотации
	"supernew": false, // разрешить использование new для встроенных объектов
	"validthis": false, // разрешить this за пределами области функций

	/*
	 * окружения
	 */

	"browser": false, // добавлять глобальные объекты браузера
	"couch": false, // добавлять глобальные объекты CouchDB
	"devel": false, // добавлять глобальные объекты отладки
	"dojo": false, // добавлять глобальные объекты Dojo Toolkit
	"jquery": true, // добавлять глобальные объекты jQuery
	"mootools": false, // добавлять глобальные объекты MooTools
	"node": false, // добавлять глобальные объекты Node.js
	"nonstandard": false, // добавлять нестандартные глобальные функции (типа escape/unescape)
	"prototypejs": false, // добавлять глобальные объекты Prototype JS
	"rhino": false, // добавлять глобальные объекты Rhino
	"worker": false, // добавлять глобальные объекты Web Worker
	"wsh": false, // добавлять глобальные объекты Windows Script Host
	"yui": false, // добавлять глобальные объекты YUI

	/*
	 * устаревшие возможности
	 */

	"nomen": true, // запретить использование подчёркивания в начале и в конце переменных
	"onevar": true, // разрешить использовать только один var в функциях
	"passfail": false, // прекращать поиск ошибок дальше по коду, если найдена хотя бы одна
	"white": true, // использовать жёсткий стиль кодирования (пробелов, переносов строк и т.д.)

	/*
	 * недокументированное
	 */

	"maxerr": 32 // максимальное количество ошибок для файла

}
